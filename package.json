{
  "name": "100ms_edtech_template",
  "version": "0.1.3",
  "private": true,
  "dependencies": {
    "@100mslive/hms-video-react": "^0.2.39",
    "@craco/craco": "^6.1.1",
    "@material-ui/core": "^4.11.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "http-server": "^0.12.3",
    "logrocket": "^1.0.15",
    "logrocket-react": "^4.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start:build": "http-server build",
    "start": "craco start",
    "build": "craco build --profile",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-curly-brace-presence": [
        2,
        {
          "props": "never"
        }
      ],
      "react/self-closing-comp": [
        "error",
        {
          "component": true,
          "html": false
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.1.0",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.4"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "arrowParens": "avoid"
  }
}
